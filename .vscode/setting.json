  "editor.formatOnSave": true, 
  "editor.formatOnType": true,
  "editor.formatOnPaste": true, 
  "editor.codeActionsOnSave": { 
      "source.fixAll.eslint": true
  },
  "eslint.enable": true, 
  "eslint.validate": [ 
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "vue",
      "html"
  ]
}
